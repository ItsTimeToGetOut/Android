<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#000000"
    >

    <TextView
        android:id="@+id/a"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:text="v"
        android:textSize="15dp"
        android:textColor="#777777"
        android:gravity="center"
        android:background="#000000"
        android:layout_above="@id/a1"
        />



    <TextView
        android:id="@+id/a1"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:text="7"
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="@color/nos"
        android:layout_above="@id/a32"
        />
    <TextView
        android:id="@+id/a2"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:text="8"
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="@color/nos"
        android:layout_toRightOf="@id/a1"
        android:layout_above="@id/a32"
        />

    <TextView
        android:id="@+id/a3"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:text="9"
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="@color/nos"
        android:layout_toRightOf="@id/a2"
        android:layout_above="@id/a32"
        />






    <TextView
        android:id="@+id/a31"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:text="รท"
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="#333333"
        android:layout_toRightOf="@id/a3"
        android:layout_below="@id/a"
        />
    <TextView
        android:id="@+id/a331"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:text="x"
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="#333333"
        android:layout_toRightOf="@id/a3"
        android:layout_below="@id/a31"
        />
    <TextView
        android:id="@+id/a332"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:text="-"
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="#333333"
        android:layout_toRightOf="@id/a3"
        android:layout_below="@id/a331"
        />
    <TextView
        android:id="@+id/a333"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:text="+"
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="#333333"
        android:layout_toRightOf="@id/a3"
        android:layout_below="@id/a332"
        />
    <TextView
        android:id="@+id/a334"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:text="="
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="#555555"
        android:layout_toRightOf="@id/a3"
        android:layout_below="@id/a333"
        />







    <TextView
        android:id="@+id/a32"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:text="4"
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="@color/nos"
        android:layout_above="@id/a4"
        />

    <TextView
        android:id="@+id/a33"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:text="5"
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="@color/nos"
        android:layout_above="@id/a4"
        android:layout_toRightOf="@id/a32"
        />

    <TextView
        android:id="@+id/a34"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:text="6"
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="@color/nos"
        android:layout_above="@id/a4"
        android:layout_toRightOf="@id/a33"
        />

    <TextView
        android:id="@+id/a4"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:text="1"
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="@color/nos"
        android:layout_above="@id/a7"
        />

    <TextView
        android:id="@+id/a5"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:text="2"
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="@color/nos"
        android:layout_toRightOf="@id/a4"
        android:layout_above="@id/a8"
        />

    <TextView
        android:id="@+id/a6"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:text="3"
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="@color/nos"
        android:layout_toRightOf="@id/a5"
        android:layout_above="@id/a9"
        />

    <TextView
        android:id="@+id/a7"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:text="."
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="@color/nos"
        android:layout_alignParentBottom="true"
        />

    <TextView
        android:id="@+id/a8"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:text="0"
        android:textSize="50sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="@color/nos"
        android:layout_toRightOf="@id/a7"
        android:layout_alignParentBottom="true"
        />

    <TextView
        android:id="@+id/a9"
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:text="DEL"
        android:textSize="30sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="@color/nos"
        android:layout_toRightOf="@id/a8"
        android:layout_alignParentBottom="true"

        />

    <TextView
        android:id="@+id/zz"
        android:layout_width="90dp"
        android:layout_height="400dp"
        android:text=""
        android:textSize="30sp"
        android:textColor="#AAAAAA"
        android.padding="9dp"
        android:gravity="center"
        android:background="#FFA500"
        android:layout_toRightOf="@id/a331"
        android:layout_alignParentBottom="true"

        />
</RelativeLayout>